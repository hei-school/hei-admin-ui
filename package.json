{
  "name": "hei-admin-ui",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:open": "concurrently \"npm run dev\" \"npx cypress open\"",
    "test:e2e": "cypress run --spec cypress/e2e/**/*",
    "test:unit": "cypress run --component",
    "test:e2e:ci": "concurrently \"npm run dev\" \"npm run test:e2e\"",
    "test:unit:ci": "npm run test:unit && npm run test:unit"
  },
  "dependencies": {
    "@haapi/typescript-client": "^1.38.0",
    "@react-admin/ra-enterprise": "^7.0.0",
    "@react-admin/ra-navigation": "^4.5.0",
    "@react-admin/ra-rbac": "^4.3.0",
    "aws-amplify": "^6.0.28",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "mime": "^4.0.3",
    "ra-language-french": "^4.16.15",
    "react": "^18.2.0",
    "react-admin": "^4.16.15",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.34",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.2.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
